      <script type="text/javascript">

      jQuery.ajaxSettings.traditional = true;

      var audio = null;
      var trackNum = 0;
      var artist = null;

      function playSong(artist) {
        var req = new XMLHttpRequest();
        req.open('GET', 'https://api.spotify.com/v1/search?type=track&q=' + encodeURIComponent(artist), true);
        req.onreadystatechange = function() {
          if (req.readyState == 4 && req.status == 200){
            var data = JSON.parse(req.responseText);
            stop();
            imageURL = data.tracks.items[trackNum].album.images[1].url;
            externalURL = data.tracks.items[trackNum].artists[0].external_urls.spotify;
            $(".album_cover").append("<a href="+externalURL+"><img src="+imageURL+"></a>");
            audio = new Audio(data.tracks.items[trackNum].preview_url);
            audio.play();
          }
        };
        req.send(null);
      }

      function pauseMusic() {
        if (audio == undefined) {
        }
        else {
          audio.pause();
        }
      }

      function newArtist() {
        artist = "linkin park";//$("#artist").val();
        playSong(artist);
      }

      function info(txt) {
        $("#info").text(txt);
      }

      function initUI() {
        $("#artist").on('keydown', function(evt) {
          if (evt.keyCode == 13) {
            newArtist();
          }
        });
        $(".play").on("click", function() {
          if (($("#artist").val() != artist || (audio != undefined && audio.paused))) {
            if(audio != undefined)
              audio.pause();
            $(".album_cover").empty();
            trackNum = 0;
            newArtist();
          }
        });
        $(".pause").on("click", function(){
          pauseMusic();
        });
        $(".next").on("click", function(){
          trackNum++;
          audio.pause();
          $(".album_cover").empty();
          playSong(artist);
        });
        $(".prev").on("click", function(){
          if(trackNum != 0) {
            trackNum--;
            audio.pause();
            $("album_cover").empty();
            playSong(artist);
          }
        })
      }

      $(document).ready(function() {
        initUI();
      });

      </script>

    </body>
